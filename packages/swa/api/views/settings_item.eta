<tr>
  <td class="text-xs lg:text-sm truncate"><%= it.settingName %></td>
  <td class="text-xs lg:text-sm truncate"><%~ it.settingValue %></td>
  <td>
    <button class="btn btn-primary btn-sm" hx-get="/api/settings/<%= it.settingName %>/edit" hx-trigger="edit" onClick="htmx.trigger(this, 'edit')">
      Edit
    </button>
  </td>
</tr>