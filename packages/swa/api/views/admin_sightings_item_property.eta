<tr>
  <td class="text-xs lg:text-sm"><%= it.propertyName %></td>
  <td class="text-xs lg:text-sm truncate"><%~ it.propertyValue %></td>
  <td>
    <button class="btn btn-primary btn-sm" hx-get="/api/panel/sightings/<%= it.sightingId %>/properties/<%= it.propertyName %>/edit" hx-trigger="edit" onClick="htmx.trigger(this, 'edit')">
      Edit
    </button>
  </td>
</tr>